{
  "name": "linkops-frontend",
  "version": "1.0.0",
  "description": "LinkOps Frontend - AI Command Center Interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",

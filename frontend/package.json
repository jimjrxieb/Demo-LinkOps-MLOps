    "eslint-plugin-vue": "^10.3.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
